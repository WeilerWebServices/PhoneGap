{
  "name": "phonegap-day",
  "version": "1.1.0",
  "description": "Website for PhoneGap Day",
  "main": "index.js",
  "scripts": {
    "webpack": "node -r babel-register node_modules/.bin/webpack -p --config=webpack.config.js --progress --colors",
    "dev": "node -r babel-register node_modules/.bin/webpack-dev-server -p --config=webpack.config.js --progress --colors",
    "build" : "npm run build:production && npm run build:images",
    "build:images": "imagemin ./img ./docs/img",
    "build:production": "npm run webpack && npm run build:images",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phonegap/phonegap-day"
  },
  "keywords": [
    "phonegap"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/phonegap/phonegap-day/issues"
  },
  "homepage": "https://github.com/phonegap/phonegap-day",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "circular-json": "^0.3.0",
    "css-loader": "^0.23.0",
    "history": "1.17.0",
    "imagemin-cli": "^2.0.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.1",
    "scroll-behavior": "^0.3.0",
    "static-site-generator-webpack-plugin": "^1.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
